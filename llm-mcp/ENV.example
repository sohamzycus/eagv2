# Copy this to .env (same directory) and fill in values

# Telegram
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
MCP_SSE_HOST=127.0.0.1
MCP_SSE_PORT=8765
MCP_SSE_PATH=/sse

# Local Inbox (HTTP + SSE)
INBOX_HTTP_HOST=127.0.0.1
INBOX_HTTP_PORT=8780
INBOX_MCP_HOST=127.0.0.1
INBOX_MCP_PORT=8781
INBOX_MCP_PATH=/inbox

# Networking (set to 1 to disable SSL verification for corporate proxies)
INSECURE_SSL=0

# Google OAuth
GOOGLE_TOKEN_PATH=./secrets/token.json
GOOGLE_CREDENTIALS_PATH=./secrets/credentials.json
GMAIL_DEFAULT_TO=

# Gemini (if used)
GEMINI_API_KEY=

